size,\n      dataSource: [\n        {\n          key: '1',\n          name: '胡彦斌',\n          age: 32,\n          address: '西湖区湖底公园1号',\n        },\n        {\n          key: '2',\n          name: '胡彦祖',\n          age: 42,\n          address: '西湖区湖底公园1号',\n        },\n      ],\n      columns: [\n        {\n          title: '姓名',\n          dataIndex: 'name',\n          key: 'name',\n        },\n        {\n          title: '年龄',\n          dataIndex: 'age',\n          key: 'age',\n        },\n        {\n          title: '住址',\n          dataIndex: 'address',\n          key: 'address',\n        },\n      ],\n    };\n  },\n}\n</script>\n\n"],"sourceRoot":""}]}
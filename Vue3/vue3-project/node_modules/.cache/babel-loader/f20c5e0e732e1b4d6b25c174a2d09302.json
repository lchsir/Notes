E,iBAAwB,IAAI,CAAC,IAAL,CAAU,K,CAAlC,EAAuC;AAAA;AAAvC,cAAuC,UAAvC,CAA8C,CAA9C;AAAA,W,CAD+D;;;;AAAA,SAAjE,E;;AAAA,S;OAEoB,CAFpB,E;;AAAA,OAAqD,EAAlC;AAAA,K,CAGN;;;;AAAA,GAJf,CAKI,CANN,C","sourcesContent":["<template>\n  <div>\n    <a-breadcrumb>\n      <a-breadcrumb-item v-for=\"(item, index) in breadList\" :key=\"index\">\n        <a :href=\"item.path\">{{ item.meta.title }}</a>\n      </a-breadcrumb-item>\n    </a-breadcrumb>\n  </div>\n</template>\n\n<script>\nimport { reactive, toRefs, watch, nextTick } from 'vue';\nimport { useRouter, useRoute } from \"vue-router\";\nexport default {\n  setup() {\n    const router = useRouter()\n    const route = useRoute()\n\n    const state = reactive({\n      breadList: []\n    })\n\n    watch(() => route.path, () => {\n      nextTick(() => {\n        getBreadcrumb()\n      })\n    }, {\n      immediate: true\n    })\n\n    const getBreadcrumb = () => {\n      const { matched } = router.currentRoute.value\n      const matches = matched.filter((item) => item.meta && item.meta.title)\n      state.breadList = matches.filter((item) => item.meta && item.meta.title);\n    }\n\n    return {\n      ...toRefs(state),\n      getBreadcrumb\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n\n</style>"],"sourceRoot":""}]}
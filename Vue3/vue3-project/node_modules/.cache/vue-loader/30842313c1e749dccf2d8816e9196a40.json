   </template>\n    </template>\n  </div>\n</template>\n\n<script>\nimport { onMounted, reactive, toRefs } from \"vue\"\nimport {\n  PieChartOutlined,\n  MailOutlined,\n  DesktopOutlined,\n  InboxOutlined,\n  AppstoreOutlined,\n} from '@ant-design/icons-vue';\n\nexport default {\n  name: 'SidebarItem',\n  props: {\n    menuData: {\n      type: Array,\n      default: () => ([])\n    }\n  },\n  components: {\n    PieChartOutlined,\n    MailOutlined,\n    DesktopOutlined,\n    InboxOutlined,\n    AppstoreOutlined\n  },\n  setup(props) {\n\n    const state = reactive({\n      menuList: []\n    })\n\n    onMounted(() => {\n      getMenuList()\n    })\n\n    const getMenuList = () => {\n      state.menuList = props.menuData\n    }\n    \n    return {\n      ...toRefs(state)\n    }\n  }\n}\n</script>\n\n\n<style lang=\"scss\" scoped>\n:deep(.ant-menu-item-selected) {\n  background: #1890fd;\n  .ant-menu-title-content {\n    .routeLinkActive {\n      // color: #FFF;\n      color: red;\n    }\n  }\n}\n</style>"]}]}